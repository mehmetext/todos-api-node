{
  "name": "todos-api-node",
  "version": "0.0.1",
  "description": "Todos API Node",
  "scripts": {
    "build": "tsc -p .",
    "build:dev": "tsc -p . --watch",
    "start": "node dist/server.js",
    "start:dev": "node --watch dist/server.js",
    "dev": "concurrently \"pnpm build:dev\" \"pnpm start:dev\"",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "prisma:deploy": "dotenv -e .env.local -- prisma migrate deploy"
  },
  "keywords": [
    "todos",
    "api",
    "node"
  ],
  "author": "Mehmet Konuk√ßu",
  "license": "MIT",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/pg": "^8.11.10",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^7.4.2"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.13.1",
    "prisma": "^5.22.0",
    "uuid": "^11.0.2",
    "zod": "^3.23.8"
  },
  "_moduleAliases": {
    "@": "dist/app"
  }
}
